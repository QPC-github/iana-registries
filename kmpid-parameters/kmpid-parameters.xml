<?xml-stylesheet type="text/xsl" href="kmpid-parameters.xsl"?>
<?oxygen RNGSchema="kmpid-parameters.rng" type="xml"?>
<registry xmlns="http://www.iana.org/assignments" id="kmpid-parameters">
  <title>KMPID Parameters</title>
  <created>2005-10-13</created>
  <updated>2006-08-21</updated>

  
  <registry id="kmpid-parameter-values">
    <title>KMPID Parameter Values</title>
    <xref type="rfc" data="rfc4567"/>
    <registration_rule>Specification Required</registration_rule>
    <expert>Unassigned</expert>
    <record>
      <value>mikey</value>
      <description>Multimedia Internet KEYing</description>
      <purpose>Usage of MIKEY with the key-mgmt-att-field
        attribute and the keymgmt RTSP header</purpose>
      <xref type="rfc" data="rfc3830">RFC3830, Section 7</xref>
      <xref type="rfc" data="rfc4567"/>
      <note>This registration implies that the protocol identifier,
        KMPID, name space will be shared between SDP and RTSP.</note>
    </record>
  </registry>

  <people/>
</registry>
