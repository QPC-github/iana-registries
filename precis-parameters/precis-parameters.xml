<registry xmlns="http://www.iana.org/assignments" id="precis-parameters">
  <title>Preparation and Comparison of Internationalized Strings (PRECIS) Parameters</title>
  <created>2015-03-06</created>
  <updated>2015-04-24</updated>
  
  <registry id="base-classes">
    <title>PRECIS Base Classes</title>
    <xref type="draft" data="RFC-ietf-precis-framework-23"/>
    <registration_rule>RFC Required</registration_rule>
    <record date="2015-03-06">
      <value>FreeformClass</value>
      <description>A sequence of letters, numbers, symbols, spaces, and 
        other code points that is used for free-form strings.</description>
      <xref type="draft" data="RFC-ietf-precis-framework-23">RFC-ietf-precis-framework-23, Section 4.3</xref> 
    </record>
    <record date="2015-03-06">
      <value>IdentifierClass</value>
      <description>A sequence of letters, numbers, and symbols that is 
        used to identify or address a network entity.</description>
      <xref type="draft" data="RFC-ietf-precis-framework-23">RFC-ietf-precis-framework-23, Section 4.2</xref> 
    </record>
  </registry>

