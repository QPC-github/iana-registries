<?xml version="1.0"?>
<grammar xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes" ns="http://www.iana.org/assignments">

  <include href="../_support/iana-registry.rng"/>

  

  

  <define name="desc">
    <element name="registry">
      <ref name="registryMeta"/>
      <zeroOrMore>
        <element name="record">
          <optional><attribute name="date"><ref name="genericDate"/></attribute></optional>
          <optional><attribute name="updated"><ref name="genericDate"/></attribute></optional>
          <element name="value"><ref name="genericRange"/></element>
          <element name="description"><text/></element>
          <optional><element name="dtls"><text/></element></optional>
          <ref name="references"/>
        </element>
      </zeroOrMore>
    </element>
  </define>
  
  
  <!-- this is for the TLS Extractor Label Registry -->
  
  <define name="extractor">
    <element name="registry">
      <ref name="registryMeta"/>
      <zeroOrMore>
        <element name="record">
          <optional><attribute name="date"><ref name="genericDate"/></attribute></optional>
          <optional><attribute name="updated"><ref name="genericDate"/></attribute></optional>
          <element name="description"><text/></element>
          <optional><element name="dtls"><text/></element></optional>
          <ref name="references"/>
          <optional><element name="note"><text/></element></optional>
        </element>
      </zeroOrMore>
    </element>
  </define>


  <start>
    <element name="registry">
      <ref name="registryMeta"/>
      
      <ref name="desc"/>
      
      <element name="registry">
        <ref name="registryMeta"/>
        <zeroOrMore>
          <element name="record">
            <optional><attribute name="date"><ref name="genericDate"/></attribute></optional>
            <optional><attribute name="updated"><ref name="genericDate"/></attribute></optional>
            <element name="value">
              <text/>
            </element>
            <element name="description"><text/></element>
            <optional><element name="dtls"><text/></element></optional>
            <ref name="references"/>
          </element>
        </zeroOrMore>
      </element>
      <ref name="desc"/>
      <ref name="desc"/>
      <ref name="desc"/>
      <ref name="desc"/>
      <ref name="desc"/>
      <ref name="desc"/>
      
      <ref name="desc"/>
      
      <ref name="desc"/>
      
      <ref name="desc"/>
      
      <ref name="desc"/>
      <ref name="extractor"/>
      
      <ref name="desc"/>
      <ref name="desc"/> <!-- Heartbeat Message Types -->
      <ref name="desc"/>  <!-- Heartbeat Modes -->
      <ref name="people"/>
    </element>
  </start>

</grammar>
