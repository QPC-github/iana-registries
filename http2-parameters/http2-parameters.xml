    <record date="2015-02-20">
      <value>0x0</value>
      <description>DATA</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.1</xref>
    </record>
    <record date="2015-02-20">
      <value>0x1</value>
      <description>HEADERS</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.2</xref>
    </record>
    <record date="2015-02-20">
      <value>0x2</value>
      <description>PRIORITY</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.3</xref>
    </record>
    <record date="2015-02-20">
      <value>0x3</value>
      <description>RST_STREAM</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.4</xref>
    </record>
    <record date="2015-02-20">
      <value>0x4</value>
      <description>SETTINGS</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.5</xref>
    </record>
    <record date="2015-02-20">
      <value>0x5</value>
      <description>PUSH_PROMISE</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.6</xref>
    </record>
    <record date="2015-02-20">
      <value>0x6</value>
      <description>PING</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.7</xref>
    </record>
    <record date="2015-02-20">
      <value>0x7</value>
      <description>GOAWAY</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.8</xref>
    </record>
    <record date="2015-02-20">
      <value>0x8</value>
      <description>WINDOW_UPDATE</description>
      <xref type="draft" data="RFC-ietf-httpbis-http2-17">RFC-ietf-httpbis-http2-17, Section 6.9</xref>
